{
  "expo": {
    "name": "Divoora",
    "slug": "divoora-task",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/divoora-icon.png",
    "scheme": "divooratask",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "backgroundColor": "#C62828",
      "image": "./assets/icons/divoora-icon.png",
      "resizeMode": "cover"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/divoora-icon.png",
        "backgroundColor": "#C62828"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
